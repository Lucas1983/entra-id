# Environment variables for local development with Docker Compose
# Fill in your Azure Entra ID (AAD) values below. These are used by all three services.
# NOTE: docker compose automatically reads this .env file from the project root.

# Azure tenant (directory) ID, e.g. 00000000-0000-0000-0000-000000000000
AZURE_TENANT_ID=

# App registration (client) ID, e.g. 11111111-1111-1111-1111-111111111111
# Client secret value for the above app registration
WEB_APP_AZURE_CLIENT_ID=
WEB_APP_AZURE_CLIENT_SECRET=
WEB_APP_AZURE_APP_ID_URI=api://

MIDDLE_TIER_API_AZURE_CLIENT_ID=
MIDDLE_TIER_API_AZURE_CLIENT_SECRET=
MIDDLE_TIER_API_AZURE_APP_ID_URI=api://

DOWNSTREAM_API_AZURE_CLIENT_ID=
DOWNSTREAM_API_AZURE_CLIENT_SECRET=
DOWNSTREAM_API_AZURE_APP_ID_URI=api://

